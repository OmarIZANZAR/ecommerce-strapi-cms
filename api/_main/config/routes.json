{
    "routes" : [
        {
            "method" : "GET",
            "path": "/main",
            "handler": "main.index"
        },
        {
            "method": "POST",
            "path": "/checkout/pay",
            "handler": "main.createPaymentIntent"
        },
        {
            "method": "POST",
            "path": "/checkout/order",
            "handler": "main.createOrderFromCheckout"
        },
        {
            "method": "POST",
            "path": "/checkout/:id",
            "handler": "main.cartCheckout"
        }
    ]
}